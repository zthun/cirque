    "@types/lodash": "^4.14.196",
    "@types/node": "^20.4.5",
    "@types/selenium-webdriver": "^4.1.15",
    "@vitest/coverage-istanbul": "^0.33.0",
    "@zthun/lint-janitor": "^14.2.0",
    "@zthun/lint-janitor-config": "^14.2.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lerna": "^7.1.4",
    "less": "^4.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6",
