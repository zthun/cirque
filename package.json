    "@originjs/vite-plugin-global-style": "^1.0.3",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.1",
    "@types/selenium-webdriver": "^4.1.20",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@zthun/lint-janitor": "^15.1.1",
    "@zthun/lint-janitor-config": "^15.1.1",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lerna": "^8.0.0",
    "less": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.2",
