{
  "name": "@zthun/circus",
  "version": "0.0.0",
  "description": "Welcome to the show. We hope you enjoy your stay.",
  "author": "Anthony Bonta",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/zthun/cirque"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "rimraf --glob 'dist' 'reports' '*.tgz' '*.log' 'packages/**/dist' 'packages/**/*.tgz' 'packages/**/*.log'",
    "test": "vitest --run --coverage",
    "e2e": "cucumber-js",
    "lint": "lint-janitor",
    "build": "lerna run build",
    "make": "yarn clean && yarn lint && yarn test && yarn build",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@cucumber/cucumber": "^10.0.1",
    "@originjs/vite-plugin-global-style": "^1.0.3",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.30",
    "@types/selenium-webdriver": "^4.1.22",
    "@vitest/coverage-istanbul": "^1.4.0",
    "@zthun/lint-janitor": "^16.0.0",
    "@zthun/lint-janitor-config": "^16.0.1",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lerna": "^8.1.2",
    "less": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0"
  },
  "resolutions": {
    "rollup": "npm:@rollup/wasm-node@*"
  },
  "packageManager": "yarn@4.1.1"
}
